# PostgreSQL database connection settings
POSTGRESQL_HOST=your-postgresql-host   # PostgreSQL server address
POSTGRESQL_PORT=your-postgresql-port   # PostgreSQL port (default: 5432)
POSTGRESQL_USER=your-postgresql-user   # PostgreSQL username
POSTGRESQL_PASSWORD=your-postgresql-password   # PostgreSQL password
POSTGRESQL_DBNAME=your-database-name   # Database name

# Backup directory path
DIRECTORY_BACKUP_PATH=your-backup-directory   # Local directory where backups will be stored

# S3 storage settings
S3_BUCKET_NAME=your-s3-bucket   # Name of the S3 bucket
S3_REGION=your-s3-region   # Region of your S3 bucket
S3_ACCESS_KEY=your-s3-access-key   # S3 access key for authentication
S3_SECRET_KEY=your-s3-secret-key   # S3 secret key for authentication
S3_ENDPOINT=your-s3-endpoint   # S3 endpoint URL
S3_PUBLIC_ENDPOINT=your-s3-public-endpoint   # Public S3 URL for accessing stored backups

# SMTP email settings (for backup notifications)
SMTP_HOST=your-smtp-host   # SMTP server address
SMTP_PORT=your-smtp-port   # SMTP port (465 for SSL, 587 for TLS)
SMTP_USER=your-smtp-user   # SMTP username (email address)
SMTP_PASS=your-smtp-password   # SMTP password
SMTP_SENDER_SIGN=your-email-signature   # Sender email address for notifications
